<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <context id="user">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <plugin type="soke.kernel.db.MySQLLimitPlugin"></plugin>

        <commentGenerator>
            <property name="suppressAllComments" value="false"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/test?serverTimezone=UTC"
                        driverClass="com.mysql.jdbc.Driver" password="123456" userId="root"/>

        <javaModelGenerator targetPackage="com.xpj.user.domain.model"
                            targetProject="springboot/src/main/java">
            <property name="constructorBased" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.xpj.user.mapper"
                         targetProject="springboot/src/main/resources"/>

        <javaClientGenerator targetPackage="com.xpj.user.mapper"
                             targetProject="springboot/src/main/java" type="XMLMAPPER"/>

        <table tableName="usert" domainObjectName="User">
            <!-- 			enableSelectByPrimaryKey="true" enableUpdateByPrimaryKey="true" -->
            <!-- 			enableDeleteByPrimaryKey="true"> -->
            <!-- 			<generatedKey column="id" sqlStatement="MySql" identity="true" /> -->
            <columnOverride column="crt_time" property="createTime"
                            javaType="java.time.LocalDateTime"/>
            <columnOverride column="upt_time" property="updateTime"
                            javaType="java.time.LocalDateTime"/>
        </table>

    </context>
</generatorConfiguration>